#!/bin/bash
bench_list=(
#"b19"
#"vga_lcd"
#"mgc_edit_dist" 
"mgc_matrix_mult" 
#"netcard"
#"leon2" 
#"leon3mp" 
)

script_list=(
"resyn"
"resyn2"
#"resyn2a"
"resyn3"
"compress"
"compress2"
#"resyn2rs"
#"compress2rs"
#"recadd3"
)

placer_list=(
#"Capo"
#"NTUPlace3"
#"mPL6"
#"mPL5"
#"ComPLx"
"Eh?Placer"
)

fp_dir="../200_floorplanning"

echo ""
echo "================================================================================"
echo "                                  PLACEMENT "
echo "================================================================================"
echo ""

for bench in "${bench_list[@]}"
do
	for script in "${script_list[@]}"
	do
        for placer in "${placer_list[@]}"
        do
            source ~/.bashrc
            bookshelf_dir=${fp_dir}/bookshelf-${bench}_${script}
            out_dir=${bench}_${script}_${placer}
            ./run_place.sh ${bench} ${bookshelf_dir} $placer $out_dir
        done
	done
done
